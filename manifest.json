{
  "manifest_version": 2,
  "name": "VoiceMuter",
  "description": "Sound Muters mute all anyoing and noise ads coming from any website you are curently not using!.",
  "version": "0.1",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "icons": {
    "16": "mute.png",
    "48": "mute.png",
    "128": "mute.png"
  },
  "permissions": [
    "tabs",
    "storage"
  ]
}